<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.itesla_project.eu/schema/iidm/ext/standby_automaton/1_0"
           elementFormDefault="qualified">
    <xs:element name="standbyAutomaton">
        <xs:complexType>
            <xs:attribute name="b0" use="required" type="xs:float"/>
            <xs:attribute name="standby" use="required" type="xs:boolean"/>
            <xs:attribute name="lowVoltageSetPoint" use="required" type="xs:float"/>
            <xs:attribute name="highVoltageSetPoint" use="required" type="xs:float"/>
            <xs:attribute name="lowVoltageThreshold" use="required" type="xs:float"/>
            <xs:attribute name="highVoltageThreshold" use="required" type="xs:float"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
