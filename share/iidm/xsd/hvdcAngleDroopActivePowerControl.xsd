<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.itesla_project.eu/schema/iidm/ext/hvdc_angle_droop_active_power_control/1_0"
           elementFormDefault="qualified">
    <xs:element name="hvdcAngleDroopActivePowerControl">
        <xs:complexType>
            <xs:attribute name="p0" type="xs:float" use="required"/>
            <xs:attribute name="droop" type="xs:float" use="required"/>
            <xs:attribute name="enabled" type="xs:boolean" use="required"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
